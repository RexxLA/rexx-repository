-- i-n bypass stage

pipe (addt6) gen 10 | addtest6 | console

import org.netrexx.njpipes.pipes.
class addtest6 extends stage final

method run() public

   say 'i-n : b 1, b 2 .. b 10'
   do
      addpipe (addpt6) *in0: | specs /b/ 1 1-* nextw | console

      loop forever
         line = peekto()
         output(line)
         readto()
      end

   catch StageError
      rc = rc()
   end

   say 'Ending 'rc

exit(rc*(rc<>12))
